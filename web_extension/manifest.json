{
  "manifest_version": 3,
  "name": "Baixar Tudo",
  "description": "Extensão para baixar todos os links da página em um clique.",
  "version": "1.0",
  "permissions": [
    "downloads",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
